list a ::=
    += 2 a
    += 3 b
    += 3 c
    += 1 d
    += 4 e
    += 2 f

list a uniq

foreach a {
    ($a-$a0) >> (x)
}

list a ::=
    += 1 g
    += 1 h
list a sort

foreach a {
    ($a-$a0) >> (y)
}

clear list a

list a ::=
    += 3 i
    += 2 j
list a sort uniq

foreach a {
    ($a-$a0) >> (z)
}
