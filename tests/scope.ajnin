list a ::= 0 1 2

foreach a {
    rule ru |= (g1-$a)
    ($a) --ru-- (x-$a)
}

foreach a {{
    rule ru |= (g2-$a)
    ($a) --ru-- (y-$a)
}}

foreach a {
    ($a) --ru-- (z-$a)
}
